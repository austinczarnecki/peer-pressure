{
  "manifest_version": 2,

  "name": "Peer Pressure",
  "description": "This extension keeps you from getting off task",
  "version": "1.0",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "tabs",
    "https://graph.facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js"]
    }
  ]
}